{
  "expo": {
    "expo.jsEngine": "hermes",
    "name": "fi_tech",
    "displayName": "FiTech",
    "slug": "fi_tech",
    "version": "3.0.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/fiTech_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.ander913.fitech",
      "supportsTablet": true,
      "icon": "./assets/fiTech_logo.png",
      "splash": {
        "image": "./assets/fiTech_logo.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anderson913.fi_tech"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3958ddfd-2601-450b-84fb-c1094932ac85"
      }
    },
    "plugins": [
      [
      "react-native-vision-camera",
      {
        "cameraPermissionText": "$(PRODUCT_NAME) needs access to your Camera.",
        "enableMicrophonePermission": true,
        "microphonePermissionText": "$(PRODUCT_NAME) needs access to your Microphone."
      }
    ]
    ]
  }
}
